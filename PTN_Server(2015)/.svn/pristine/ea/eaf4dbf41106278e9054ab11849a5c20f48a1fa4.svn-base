package com.nms.drive.service.bean;

public class NE {
	private int NESwitch = 0;//网元开关1，网元1开关2
	private int NEType = 0;//网元类型
	private String NEAddr = "";//网元NE地址1，网元NE地址2
	private String NEIp = "";//网元的IP地址1，网元的IP地址2，网元的IP地址3，网元的IP地址4
	private String NESubnetScreen = "";//网元的子网屏蔽1，网元的子网屏蔽2，网元的子网屏蔽3，网元的子网屏蔽4
	private String NEEthGateway = "";//网元的以太网网关1，网元的以太网网关2，网元的以太网网关3，网元的以太网网关4
	private int NECardReserveType = 0;//网元盘类型
	 
	public int getNESwitch() {
		return NESwitch;
	}
	public void setNESwitch(int switch1) {
		NESwitch = switch1;
	}
	public int getNEType() {
		return NEType;
	}
	public void setNEType(int type) {
		NEType = type;
	}
	public String getNEAddr() {
		return NEAddr;
	}
	public void setNEAddr(String addr) {
		NEAddr = addr;
	}
	public String getNEIp() {
		return NEIp;
	}
	public void setNEIp(String ip) {
		NEIp = ip;
	}
	public String getNESubnetScreen() {
		return NESubnetScreen;
	}
	public void setNESubnetScreen(String subnetScreen) {
		NESubnetScreen = subnetScreen;
	}
	public String getNEEthGateway() {
		return NEEthGateway;
	}
	public void setNEEthGateway(String ethGateway) {
		NEEthGateway = ethGateway;
	}
	public int getNECardReserveType() {
		return NECardReserveType;
	}
	public void setNECardReserveType(int cardReserveType) {
		NECardReserveType = cardReserveType;
	}
}
