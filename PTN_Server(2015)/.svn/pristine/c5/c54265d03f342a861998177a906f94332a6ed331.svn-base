package com.nms.db.bean.report;

import java.io.Serializable;

/**
 * @auther stonesLi addby 20130628 for tunnel Search in lspinfo table.
 * **/
@SuppressWarnings("serial")
public class SearchTunnelInfo implements Serializable {
	private int id ; //id
	private int az ; // 判断当前lsp所对应的tunnel是 a端  中间端 z端。
	private int tunnelType ;
	private int tunnelStatus ; //判断当前tunnel 的激活状态 .1 激活 ，2 未激活  （激活过后下发设备）
	private int tunnelId ;
	private int aSiteId;
	private int zSiteId;
	private int aPortId;
	private int zPortId;
	private int frontLableValue;
	private int backLableValue;
	private int atunnelbusinessid;
	private int ztunnelbusinessid;
	private int segmentId;
	private int SID;
	private int aoppositeId;
	private String aoppositeIdIP;
	private int zoppositeId;
	private String zoppositeIdIP;
	
	@Override
	public String toString(){
		StringBuffer s = new StringBuffer().append("id=").append(id)
		.append(";az=").append(az).append(";tunnelStatus=").append(tunnelStatus)
		.append(";tunnelId=").append(tunnelId).append(";aSiteId=").append(aSiteId)
		.append(";zSiteId=").append(zSiteId).append(";aPortId=").append(aPortId)
		.append(";zPortId=").append(zPortId).append(";frontLableValue=").append(frontLableValue)
		.append(";backLableValue=").append(backLableValue).append(";atunnelbusinessid=").append(atunnelbusinessid)
		.append(";ztunnelbusinessid=").append(ztunnelbusinessid).append(";segmentId=").append(segmentId)
		.append(";SID=").append(SID).append(";aoppositeId=").append(aoppositeId)
		.append(";aoppositeIdIP=").append(aoppositeIdIP).append(";zoppositeId=").append(zoppositeId)
		.append(";zoppositeIdIP=").append(zoppositeIdIP);
		return s.toString();
	}
	
	public int getTunnelType() {
		return tunnelType;
	}

	public void setTunnelType(int tunnelType) {
		this.tunnelType = tunnelType;
	}

	public String getAoppositeIdIP() {
		return aoppositeIdIP;
	}
	public void setAoppositeIdIP(String aoppositeIdIP) {
		this.aoppositeIdIP = aoppositeIdIP;
	}
	public String getZoppositeIdIP() {
		return zoppositeIdIP;
	}
	public void setZoppositeIdIP(String zoppositeIdIP) {
		this.zoppositeIdIP = zoppositeIdIP;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getAz() {
		return az;
	}
	
	public int getTunnelStatus() {
		return tunnelStatus;
	}
	public void setTunnelStatus(int tunnelStatus) {
		this.tunnelStatus = tunnelStatus;
	}
	public void setAz(int az) {
		this.az = az;
	}
	public int getTunnelId() {
		return tunnelId;
	}
	public void setTunnelId(int tunnelId) {
		this.tunnelId = tunnelId;
	}
	public int getASiteId() {
		return aSiteId;
	}
	public void setASiteId(int siteId) {
		aSiteId = siteId;
	}
	public int getZSiteId() {
		return zSiteId;
	}
	public void setZSiteId(int siteId) {
		zSiteId = siteId;
	}
	public int getAPortId() {
