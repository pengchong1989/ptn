	}
	public void setAPortId(int portId) {
		aPortId = portId;
	}
	public int getZPortId() {
		return zPortId;
	}
	public void setZPortId(int portId) {
		zPortId = portId;
	}
	public int getFrontLableValue() {
		return frontLableValue;
	}
	public void setFrontLableValue(int frontLableValue) {
		this.frontLableValue = frontLableValue;
	}
	public int getBackLableValue() {
		return backLableValue;
	}
	public void setBackLableValue(int backLableValue) {
		this.backLableValue = backLableValue;
	}
	public int getAtunnelbusinessid() {
