	}
	public void setAtunnelbusinessid(int atunnelbusinessid) {
		this.atunnelbusinessid = atunnelbusinessid;
	}
	public int getZtunnelbusinessid() {
		return ztunnelbusinessid;
	}
	public void setZtunnelbusinessid(int ztunnelbusinessid) {
		this.ztunnelbusinessid = ztunnelbusinessid;
	}
	public int getSegmentId() {
		return segmentId;
	}
	public void setSegmentId(int segmentId) {
		this.segmentId = segmentId;
	}
	public int getSID() {
		return SID;
	}
	public void setSID(int sid) {
