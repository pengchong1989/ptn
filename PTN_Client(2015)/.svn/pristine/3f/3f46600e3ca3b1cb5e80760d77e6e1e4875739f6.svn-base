<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nms.db.dao.ptn.qos.QosRelevanceMapper" >
  <resultMap id="BaseResultMap" type="QosRelevance" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="objType" property="objType" jdbcType="VARCHAR" />
    <result column="objId" property="objId" jdbcType="INTEGER" />
    <result column="siteId" property="siteId" jdbcType="INTEGER" />
    <result column="qosGroupId" property="qosGroupId" jdbcType="INTEGER" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, objType, objId, siteId, qosGroupId
  </sql>
  <insert id="insert" parameterType="QosRelevance" >
    insert into qosrelevance (id, objType, objId, 
      siteId, qosGroupId)
    values (#{id,jdbcType=INTEGER}, #{objType,jdbcType=VARCHAR}, #{objId,jdbcType=INTEGER}, 
      #{siteId,jdbcType=INTEGER}, #{qosGroupId,jdbcType=INTEGER})
  </insert>
  <update id="update" parameterType="QosRelevance" >
    update qosrelevance
    set qosGroupId = #{qosGroupId,jdbcType=INTEGER}
    where objType = #{objType,jdbcType=VARCHAR} and id = #{id,jdbcType=INTEGER} and objId = #{objId,jdbcType=INTEGER}
  </update>
   <select id="queryByCondition" resultType="QosRelevance" parameterType="QosRelevance" >
    select 
    <include refid="Base_Column_List" />
    from qosrelevance where 1=1
     <if test="id>0 ">
        and id =#{id}
     </if>
     <if test="objType!=null and objType!='' ">
        and objType =#{objType}
     </if>
      <if test="objId>0 ">
        and objid =#{objId}
     </if>
     <if test="siteId>0 ">
        and siteId =#{siteId}
     </if>
     <if test="qosGroupId>0 ">
        and qosGroupId =#{qosGroupId}
     </if>
  </select>
</mapper>