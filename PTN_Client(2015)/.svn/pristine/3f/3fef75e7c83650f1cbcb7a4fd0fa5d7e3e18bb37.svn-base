package com.nms.service.bean;

import java.util.concurrent.ConcurrentHashMap;

/**
 * 
 * 处理长期性能任务的数据对象
 * @author zk
 */
public class PerformanceObjectService {
	
	private  ConcurrentHashMap<String,byte[]> hisPerformanceMap = new ConcurrentHashMap<String, byte[]>();//用于存储性能数据

	
	public ConcurrentHashMap<String, byte[]> getHisPerformanceMap() {
		return hisPerformanceMap;
	}

	public void setHisPerformanceMap(
			ConcurrentHashMap<String, byte[]> hisPerformanceMap) {
		this.hisPerformanceMap = hisPerformanceMap;
	}
	

	
}
