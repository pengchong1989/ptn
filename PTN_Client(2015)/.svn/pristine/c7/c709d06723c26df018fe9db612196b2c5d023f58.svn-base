   	and siteId = #{siteId}
   </if>
   <if test="slotIdList != null and slotIdList.size() > 0">
    and slotId in
   <foreach item="item" index="index" collection="slotIdList" open="(" separator="," close=")">  
 	 #{item}  
 	</foreach>
